{
    "googleSheetsConfig": {
        "spreadsheetId": "YOUR_GOOGLE_SHEET_ID_HERE",
        "worksheetName": "Sheet1",
        "range": "A:Z",
        "skipHeaderRows": 1,
        "dataStructure": {
            "type": "indicator_value_pairs",
            "indicatorColumn": "Indicator",
            "valueColumn": "Value",
            "periodColumn": "Period",
            "orgUnitColumn": "OrgUnit"
        },
        "authentication": {
            "type": "service_account",
            "note": "Ensure your Google Service Account has read access to the spreadsheet"
        }
    },
    "processingConfig": {
        "dataValidation": {
            "requireIndicatorName": true,
            "requireNumericValue": true,
            "skipEmptyRows": true,
            "logMissingMappings": true
        },
        "transformations": {
            "normalizeIndicatorNames": true,
            "convertValuesToNumbers": true,
            "handleMissingData": "default_to_zero"
        }
    }
}
